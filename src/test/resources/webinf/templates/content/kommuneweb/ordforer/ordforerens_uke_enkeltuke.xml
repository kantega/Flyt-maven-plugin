        <attribute name="aar" title="År" type="number" mandatory="true" default="YEAR"/>
        <attribute name="mandag"  title="Mandag" type="htmltext" mandatory="false"/>
        <attribute name="tirsdag" title="Tirsdag" type="htmltext" mandatory="false"/>
        <attribute name="onsdag" title="Onsdag" type="htmltext" mandatory="false"/>
        <attribute name="torsdag" title="Torsdag" type="htmltext" mandatory="false"/>
        <attribute name="fredag" title="Fredag" type="htmltext" mandatory="false"/>
        <attribute name="lordag" title="Lørdag" type="htmltext" mandatory="false"/>
        <attribute name="sondag" title="Søndag" type="htmltext" mandatory="false"/>
    </attributes>
</template>
